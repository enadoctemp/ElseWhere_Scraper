{
  "manifest_version": 3,
  "name": "Elsewhere Scraper",
  "description": "Cross-layer intelligent web data scraper.",
  "version": "1.1",
  "permissions": ["tabs", "scripting", "activeTab", "storage"],
  "host_permissions": ["<all_urls>"],
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
      "16": "fox.png",
      "48": "fox.png",
      "128": "fox.png"
    }
  },
  "icons": {
    "16": "fox.png",
    "48": "fox.png",
    "128": "fox.png"
  }
}